{
  "format_version": "1.10.0",
  "minecraft:attachable": {
    "description": {
      "identifier": "heavy:piston_boots.player",
      "item": {
        "heavy:piston_boots": "query.owner_identifier == 'minecraft:player'"
      },
      "materials": {
        "default": "armor",
        "enchanted": "armor_enchanted"
      },
      "textures": {
        "default": "textures/armor/piston_boots/piston_boots",
        "enchanted": "textures/misc/enchanted_item_glint"
      },
      "geometry": {
        "default": "geometry.piston_boots_off",
        "piston_on": "geometry.piston_boots_on"
      },
      "scripts": {
        "parent_setup": "variable.boot_layer_visible = 0.0;"
      },
      "render_controllers": [
        "controller.render.piston_on"
      ]
    }
  }
}